public class a07 {
    //print a 12x12 multiplication table
    public static void main(String[] args) {
        for (int i = 1; i <=12; i++){
            for (int j = 1; j <= 12; j++){

                System.out.print(" " + i*j); //same row
            }
            System.out.println("\n"); //next row
        }
    }
}
