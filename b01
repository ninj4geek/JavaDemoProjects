import java.util.*;

public class MyClass {
    public static void main(String args[]) {
    //b01 - Write a function that returns the largest element in a list.
    Scanner input = new Scanner(System.in);
    List<String> list = new ArrayList<>();
    
    System.out.println("Please add your list item: ");
    list.add(input.nextLine());
        System.out.println("Please add your list item: ");
    list.add(input.nextLine());
        System.out.println("Please add your list item: ");
    list.add(input.nextLine());
        System.out.println("Please add your list item: ");
    list.add(input.nextLine());
        System.out.println("Please add your list item: ");
    list.add(input.nextLine());
        System.out.println("Please add your list item: ");
    list.add(input.nextLine());
    
        
    System.out.println("Largest item is: "+Collections.max(list));
    }
}
