public class a08 {
    //print all primes up to 100
    public static void main(String[] args) {
        for (int i = 2; i <= 99999; i++){
            if (isPrime(i) == true){
                System.out.println(i + " is prime ");
            }
        }
    }

    public static boolean isPrime(int i){
        //brute force method: check % == 0 for every smaller number

        boolean flag = false;
        for (int j = 2; j < i; ++j){
            if (i % j == 0){
                flag = true;
                break;
            }
        }
        if (flag == true){
            return false;
        }
        return true;
    }
}
